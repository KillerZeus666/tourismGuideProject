CREATE OR REPLACE PROCEDURE generar_datos_temp IS
BEGIN
  FOR i IN 1..50 LOOP
    INSERT INTO TempCarga VALUES (
      'Guia_' || i,
      CASE MOD(i,2) WHEN 0 THEN 'M' ELSE 'F' END,
      1000 + i,
      'Ciudad_' || TRUNC(DBMS_RANDOM.VALUE(1,10)),
      'Pais_' || TRUNC(DBMS_RANDOM.VALUE(1,5)),
      SYSDATE - TRUNC(DBMS_RANDOM.VALUE(0,365)),
      TRUNC(DBMS_RANDOM.VALUE(50,300))
    );
  END LOOP;
  COMMIT;
END;
/